<h2>Edit User</h2>

<%= render "form.html", changeset: @changeset,
                        action: user_path(@conn, :update, @user),
                        types: @types,
                        password_editable: @user == @current_user %>

<span><%= link "Back", to: user_path(@conn, :index), class: "btn btn-default" %></span>
